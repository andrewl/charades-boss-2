<?xml version='1.0' encoding='utf-8'?>
<widget id="net.shiningweb.charades_boss" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Charades Boss</name>
    <description>
        It's Charades Boss!
    </description>
    <author email="charades_boss@shiningweb.net" href="http://shiningweb.net">
        Andrew Larcombe
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icons/charadesbossicon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
